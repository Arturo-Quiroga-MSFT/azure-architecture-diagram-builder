  "workflow": [
    {
      "step": 1,
      "description": "User accesses the e-commerce site; CDN accelerates content delivery.",
      "services": [
        "svc-cdn",
        "svc-frontend"
      ]
    },
    {
      "step": 2,
      "description": "Customer browses, searches products, builds cart; app gateway routes API calls.",
      "services": [
        "svc-frontend",
        "svc-appgw",
        "svc-k8s"
      ]
    },
    {
      "step": 3,
      "description": "Product catalog, cart, and session state accessed via Cosmos DB and Redis.",
      "services": [
        "svc-k8s",
        "svc-cosmos",
        "svc-redis"
      ]
    },
    {
      "step": 4,
      "description": "Users perform faceted search via Azure Cognitive Search.",
      "services": [
        "svc-k8s",
        "svc-search"
      ]
    },
    {
      "step": 5,
      "description": "User requests trigger personalized product recommendations.",
      "services": [
        "svc-k8s",
        "svc-aml-reco"
      ]
    },
    {
      "step": 6,
      "description": "Customer checkout triggers fraud detection for transactions (sub-200ms API call).",
      "services": [
        "svc-k8s",
        "svc-aml-fraud"
      ]
    },
    {
      "step": 7,
      "description": "Order placed; details sent via Service Bus for orchestration and warehouse sync.",
      "services": [
        "svc-k8s",
        "svc-servicebus"
      ]
    },
    {
      "step": 8,
      "description": "Functions trigger inventory update webhooks to 12 warehouses.",
      "services": [
        "svc-servicebus",
        "svc-functions"
      ]
    },
    {
      "step": 9,
      "description": "If cart is abandoned, Logic Apps runs recovery/notification workflows.",
      "services": [
        "svc-servicebus",
        "svc-logicapps"
      ]
    },
    {
      "step": 10,
      "description": "All access is authenticated/authorized with Entra ID; payments leverage Key Vault PCI secrets.",
      "services": [
        "svc-k8s",
        "svc-entra",
        "svc-keyvault"
      ]
    }
  ],
  "architecturePrompt": "A Black Friday-ready e-commerce platform handling 50,000 orders/hour peak with real-time inventory sync across 12 regional warehouses, ML-powered fraud detection scoring each transaction in under 200ms, personalized recommendations engine, multi-currency payment processing with PCI-DSS compliance, abandoned cart recovery workflows, using Azure Kubernetes Service for microservices, Cosmos DB for product catalog with global distribution, Redis Cache for session and cart state, Service Bus for order orchestration, Azure Functions for inventory webhooks, Cognitive Search for faceted product search, and CDN with dynamic site acceleration"
