  "workflow": [
    {
      "step": 1,
      "description": "Data Factory executes ingestion pipelines to pull data from external sources and stage it into the lake.",
      "services": [
        "svc-adf",
        "svc-adls"
      ]
    },
    {
      "step": 2,
      "description": "Data Factory triggers an Azure Machine Learning pipeline run when fresh datasets are available.",
      "services": [
        "svc-adf",
        "svc-aml"
      ]
    },
    {
      "step": 3,
      "description": "Azure Machine Learning reads training/validation datasets from Data Lake Storage and performs preparation/feature generation.",
      "services": [
        "svc-aml",
        "svc-adls"
      ]
    },
    {
      "step": 4,
      "description": "Azure Machine Learning submits the training job to AML Managed Compute and tracks metrics and artifacts.",
      "services": [
        "svc-aml",
        "svc-aml-compute"
      ]
    },
    {
      "step": 5,
      "description": "Azure Machine Learning builds versioned training/inference images and pushes them to Container Registry for reproducible deployments.",
      "services": [
        "svc-aml",
        "svc-acr"
      ]
    },
    {
      "step": 6,
      "description": "Azure Machine Learning registers the trained model and deploys it to the AML Online Endpoint for low-latency scoring.",
      "services": [
        "svc-aml",
        "svc-aml-online"
      ]
    },
    {
      "step": 7,
      "description": "Clients authenticate via Microsoft Entra ID and call the AML Online Endpoint to get real-time predictions; telemetry is captured in Application Insights.",
      "services": [
        "svc-entra",
        "svc-aml-online",
        "svc-appinsights"
      ]
    },
    {
      "step": 8,
      "description": "For large datasets, Azure Machine Learning publishes the model to an AML Batch Endpoint; batch jobs read inputs from the lake and write prediction outputs back.",
      "services": [
        "svc-aml",
        "svc-aml-batch",
        "svc-adls"
      ]
    },
    {
      "step": 9,
      "description": "Secrets for connectors, registries, and data access are retrieved from Key Vault by Data Factory and Azure Machine Learning components.",
      "services": [
        "svc-kv",
        "svc-adf",
        "svc-aml"
      ]
    },
    {
      "step": 10,
      "description": "Training and scoring logs/metrics flow into Log Analytics for centralized querying, alerting, and operational monitoring.",
      "services": [
        "svc-loganalytics",
        "svc-aml",
        "svc-aml-batch"
      ]
    }
  ],
  "architecturePrompt": "A machine learning pipeline with data ingestion, training, and inference endpoints"
