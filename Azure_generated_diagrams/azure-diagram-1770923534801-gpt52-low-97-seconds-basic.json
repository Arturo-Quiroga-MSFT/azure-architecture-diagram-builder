  "workflow": [
    {
      "step": 1,
      "description": "User browses to the application URL; edge layer routes to the React SPA.",
      "services": [
        "svc-frontdoor",
        "svc-staticweb"
      ]
    },
    {
      "step": 2,
      "description": "SPA initiates sign-in and receives tokens for calling protected APIs.",
      "services": [
        "svc-staticweb",
        "svc-entra"
      ]
    },
    {
      "step": 3,
      "description": "SPA calls backend endpoints through the gateway; edge routes requests to API Management.",
      "services": [
        "svc-frontdoor",
        "svc-apim"
      ]
    },
    {
      "step": 4,
      "description": "API Management validates JWT tokens and applies rate limits before forwarding to the Node.js API.",
      "services": [
        "svc-apim",
        "svc-entra",
        "svc-appservice"
      ]
    },
    {
      "step": 5,
      "description": "Backend loads required secrets (DB and storage access) from Key Vault using managed identity.",
      "services": [
        "svc-appservice",
        "svc-keyvault"
      ]
    },
    {
      "step": 6,
      "description": "Backend reads/writes relational data in PostgreSQL to fulfill business operations.",
      "services": [
        "svc-appservice",
        "svc-postgres"
      ]
    },
    {
      "step": 7,
      "description": "Backend stores and retrieves image files in Blob Storage and returns accessible URLs/SAS to the SPA.",
      "services": [
        "svc-appservice",
        "svc-storage"
      ]
    },
    {
      "step": 8,
      "description": "SPA and API continuously emit telemetry to Application Insights for monitoring and troubleshooting.",
      "services": [
        "svc-staticweb",
        "svc-appservice",
        "svc-appinsights"
      ]
    }
  ],
  "architecturePrompt": "A web application with a React frontend, Node.js backend API, PostgreSQL database, and blob storage for images"
