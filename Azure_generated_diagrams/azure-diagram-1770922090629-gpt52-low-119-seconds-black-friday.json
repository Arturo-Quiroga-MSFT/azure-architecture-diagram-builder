  "workflow": [
    {
      "step": 1,
      "description": "Customers browse the storefront; Azure Front Door accelerates delivery and routes static assets to CDN.",
      "services": [
        "s-frontdoor",
        "s-cdn"
      ]
    },
    {
      "step": 2,
      "description": "Static images/scripts are served from CDN; cache misses are pulled from Storage Account and cached at the edge.",
      "services": [
        "s-cdn",
        "s-storage"
      ]
    },
    {
      "step": 3,
      "description": "API requests (catalog/cart/checkout) flow through API Management using Microsoft Entra ID-issued tokens for authentication.",
      "services": [
        "s-entra",
        "s-apim"
      ]
    },
    {
      "step": 4,
      "description": "Microservices on AKS fetch product data from globally distributed Cosmos DB and maintain cart/session state in Redis Cache.",
      "services": [
        "s-aks",
        "s-cosmos",
        "s-redis"
      ]
    },
    {
      "step": 5,
      "description": "Search experiences use Azure Cognitive Search for faceting/filtering and fast product discovery.",
      "services": [
        "s-aks",
        "s-search"
      ]
    },
    {
      "step": 6,
      "description": "During checkout, AKS calls AML Online Endpoint for fraud scoring within 200ms and retrieves secrets/certs from Key Vault for PCI-aligned payment flows.",
      "services": [
        "s-aks",
        "s-aml-online",
        "s-keyvault"
      ]
    },
    {
      "step": 7,
      "description": "Successful transactions publish order events to Service Bus; downstream consumers process fulfillment steps asynchronously to sustain 50,000 orders/hour.",
      "services": [
        "s-aks",
        "s-servicebus"
      ]
    },
    {
      "step": 8,
      "description": "Inventory updates are handled by Azure Functions via Service Bus messages, then written to Cosmos DB to keep 12 warehouses in sync in near real time.",
      "services": [
        "s-servicebus",
        "s-functions",
        "s-cosmos"
      ]
    },
    {
      "step": 9,
      "description": "Abandoned cart candidate events trigger Logic Apps to orchestrate recovery workflows and scheduled follow-ups.",
      "services": [
        "s-servicebus",
        "s-logicapps"
      ]
    },
    {
      "step": 10,
      "description": "Platform telemetry is collected in Application Insights and centralized in Log Analytics; Defender for Cloud provides optional posture and threat insights.",
      "services": [
        "s-appinsights",
        "s-loganalytics",
        "s-defender"
      ]
    }
  ],
  "architecturePrompt": "A Black Friday-ready e-commerce platform handling 50,000 orders/hour peak with real-time inventory sync across 12 regional warehouses, ML-powered fraud detection scoring each transaction in under 200ms, personalized recommendations engine, multi-currency payment processing with PCI-DSS compliance, abandoned cart recovery workflows, using Azure Kubernetes Service for microservices, Cosmos DB for product catalog with global distribution, Redis Cache for session and cart state, Service Bus for order orchestration, Azure Functions for inventory webhooks, Cognitive Search for faceted product search, and CDN with dynamic site acceleration"
}