  "workflow": [
    {
      "step": 1,
      "description": "Data Factory ingests and transforms raw data into Storage Account",
      "services": [
        "data-factory",
        "storage-account"
      ]
    },
    {
      "step": 2,
      "description": "Azure Machine Learning retrieves input datasets from Storage Account",
      "services": [
        "azure-machine-learning",
        "storage-account"
      ]
    },
    {
      "step": 3,
      "description": "Azure Machine Learning initiates ML training jobs via AML Managed Compute",
      "services": [
        "azure-machine-learning",
        "aml-managed-compute"
      ]
    },
    {
      "step": 4,
      "description": "AML Managed Compute trains the model and returns the trained artifact",
      "services": [
        "aml-managed-compute",
        "azure-machine-learning"
      ]
    },
    {
      "step": 5,
      "description": "Azure Machine Learning deploys trained model to AML Online Endpoint for real-time inference",
      "services": [
        "azure-machine-learning",
        "aml-online-endpoint"
      ]
    },
    {
      "step": 6,
      "description": "Azure Machine Learning deploys model to AML Batch Endpoint for scheduled inference processing",
      "services": [
        "azure-machine-learning",
        "aml-batch-endpoint"
      ]
    },
    {
      "step": 7,
      "description": "AML Online Endpoint processes real-time inference requests and sends telemetry to Application Insights",
      "services": [
        "aml-online-endpoint",
        "application-insights"
      ]
    },
    {
      "step": 8,
      "description": "AML Batch Endpoint processes batch inference requests and logs results and metrics",
      "services": [
        "aml-batch-endpoint",
        "application-insights"
      ]
    },
    {
      "step": 9,
      "description": "Azure Machine Learning and endpoints authenticate users and permissions using Microsoft Entra ID",
      "services": [
        "azure-machine-learning",
        "microsoft-entra-id"
      ]
    },
    {
      "step": 10,
      "description": "Azure Machine Learning retrieves required secrets and credentials from Key Vault to govern workspace and endpoints",
      "services": [
        "azure-machine-learning",
        "key-vault"
      ]
    }
