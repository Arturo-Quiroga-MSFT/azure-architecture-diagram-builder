  "workflow": [
    {
      "step": 1,
      "description": "Data Factory runs a scheduled ingestion pipeline to collect source data.",
      "services": [
        "svc-adf"
      ]
    },
    {
      "step": 2,
      "description": "Ingested raw data and curated datasets are written to Data Lake Storage for downstream use.",
      "services": [
        "svc-adls",
        "svc-adf"
      ]
    },
    {
      "step": 3,
      "description": "Azure Machine Learning loads curated datasets from the lake and prepares training/feature engineering steps.",
      "services": [
        "svc-aml",
        "svc-adls"
      ]
    },
    {
