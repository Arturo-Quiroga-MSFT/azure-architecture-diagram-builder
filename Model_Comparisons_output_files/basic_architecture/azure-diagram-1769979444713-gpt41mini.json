  "workflow": [
    {
      "step": 1,
      "description": "Event Hubs ingests streaming data from various sources, feeding it into Data Lake Storage for persistent raw and processed data storage.",
      "services": [
        "eventhubs1",
        "datalake1"
      ]
    },
    {
      "step": 2,
      "description": "Azure Machine Learning pulls training data from Data Lake Storage to train machine learning models securely by retrieving needed secrets from Key Vault.",
      "services": [
        "datalake1",
        "azureml1",
        "keyvault1"
      ]
    },
    {
      "step": 3,
      "description": "Trained machine learning models are containerized and pushed to Container Registry for versioning and deployment.",
      "services": [
        "azureml1",
        "containerregistry1"
      ]
    },
    {
      "step": 4,
      "description": "The model container images are deployed to Azure Machine Learning endpoints which serve real-time inference requests securely fetching necessary secrets from Key Vault.",
      "services": [
        "containerregistry1",
        "azuremlendpoint1",
        "keyvault1"
      ]
    },
    {
      "step": 5,
      "description": "Api Management acts as a secure API gateway exposing the inference endpoints and authenticating incoming requests using Microsoft Entra ID.",
      "services": [
        "apim1",
        "azuremlendpoint1",
        "entra1"
      ]
    },
    {
      "step": 6,
      "description": "Application Insights collects telemetry and monitoring data from both the API Management service and ML endpoints to provide diagnostics and performance insights.",
      "services": [
        "apim1",
        "azuremlendpoint1",
        "appinsights1"
      ]
    }
  ],
  "architecturePrompt": "A machine learning pipeline with data ingestion, training, and inference endpoints"
}